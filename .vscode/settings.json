{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.o": true,
    "**/obj": true,         // To remove "obj" folder
    "**/*.exe": true        // Comment this line out to create .exe
  },
  "files.associations": {
    "*.cpp": "cpp",
    "*.js": "javascript",
    "stdio.h": "c",
    "*.m": "cpp",
    "string": "cpp"
  }
}
